pipeline {
    agent any
    triggers {
        cron('30 23 * * 1-5')
    }
	tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "M3"
    }

	stages {
        stage('Git checkout') {
            steps {
                git 'https://github.com/galamsiva2020/mqwebservice-simple.git'
            }
        }
		stage('Maven Build'){
		    sh "mvn clean package"

			}
			
		stage('TestResults') {
	         'target/surefire-reports/*.xml'
         }
    }
    }
    
