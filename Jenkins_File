pipeline {
    agent any
    triggers {
        cron('30 23 * * 1-5')
    }
	environment {
        MAVEN_HOME = tool('M3')
    }

	stages {
        stage('Git checkout') {
            steps {
                git 'https://github.com/galamsiva2020/mqwebservice-simple.git'
            }
        }
		stage('Maven Build'){
		    sh '${MAVEN_HOME}/bin/mvn -B verify'

			}
			
		stage('TestResults') {
	         'target/surefire-reports/*.xml'
         }
    }
}
